<Page x:Class="DGToolkit.Views.Vehicles.VehicleWrapper"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DGToolkit.Views.Vehicles"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="VehicleWrapper">

    <Grid>
        <TabControl>
            <TabItem Header="Vehicle">
                <Grid>
                    <TextBox>Vehicle.meta</TextBox>
                </Grid>
            </TabItem>
            <TabItem Header="Handling">
                <Grid>
                    <TextBox>Handling.meta</TextBox>
                </Grid>
            </TabItem>
            <TabItem Header="Carcols">
                <Grid>
                    <TextBox>Carcols.meta</TextBox>
                </Grid>
            </TabItem>
            <TabItem Header="Carvariations">
                <Grid>
                    <TextBox>Carvariations.meta</TextBox>
                </Grid>
            </TabItem>
            <TabItem Header="VehicleLayouts">
                <Grid>
                    <TextBox>VehicleLayouts.meta</TextBox>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
