<Window x:Class="DGToolkit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DGToolkit"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <DockPanel>
        <TabControl>
            <TabItem Header="Audio Pack">
                <Frame Source="/Views/AudioPack/AudioPackView.xaml" HorizontalAlignment="Stretch" />
            </TabItem>
            <TabItem Header="Clothing">
                <Frame Source="/Views/Clothing/ClothingView.xaml" HorizontalAlignment="Stretch" />
            </TabItem>
            <TabItem Header="Vehicles">
                <Frame Source="/Views/Vehicles/VehicleWrapper.xaml" HorizontalAlignment="Stretch" />
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>